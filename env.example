# Configuración de Supabase
SUPABASE_URL=tu_supabase_url_aqui
SUPABASE_ANON_KEY=tu_supabase_anon_key_aqui
SUPABASE_SERVICE_ROLE_KEY=tu_supabase_service_role_key_aqui

# Configuración de JWT
JWT_SECRET=tu_jwt_secret_aqui

# Configuración de CORS
# Separa múltiples orígenes con comas
# Ejemplos:
# CORS_ORIGINS=https://lectana.vercel.app,https://www.lectana.vercel.app
# CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:5173
# CORS_ORIGINS=https://tu-dominio.com,http://localhost:3000
CORS_ORIGINS=https://lectana.vercel.app,http://localhost:3000

# Configuración del servidor
NODE_ENV=development
PORT=3000

# Configuración de Google Cloud (opcional)
GOOGLE_CLOUD_PROJECT_ID=tu_google_cloud_project_id
GOOGLE_CLOUD_KEY_FILE=path/to/your/service-account-key.json


